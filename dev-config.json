{
  "airbrake": {
    "disable": true
  },
  "dns": {
    "name": "local.pryv.net",
    "domain": "pryv.in",
    "staticDataInDomain": {
      "www": {"alias": [ { "name": "local.pryv.in" } ]},
      "sw": {"alias": [ { "name": "local.pryv.in" } ]},
      "reg": {"alias": [ { "name": "reglocal.pryv.in" } ]}
    }
  },
  "http": {
    "static": {
      "url": "https://l.rec.la:4443",
      "access": "https://local.rec.la:2443/access/SDK-VERSION-NUM/access.html"
    },
    "register": {
      "url": "https://reglocal.pryv.in:8443"
    }
  },
  "server": {
    "certsPathAndKey": "source/cert/pryv.in" ,
    "port": 8443
  },
  "net": {
    "aaservers": {
      "gandi.net-fr" : [{
        "base_name": "stact-gandi-fr-01",
        "port": 443,
        "authorization": "register-test-token"
      }],
      "gandi.net-us" : [{
        "base_name": "stact-gandi-us-01",
        "port": 443,
        "authorization": "register-test-token"
      }],
      "joyent.net-usw": [
        {
          "base_name": "stact-joyent-usw-01",
          "port": 443,
          "authorization": "register-test-token"
        }
      ]
    }
  }
}
